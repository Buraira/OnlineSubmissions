#include <iostream>
#include <queue>
#include <vector> 
#include <array>

using namespace std;

int check(queue<int>& check, int n)
{
	int som = check.size();
	int c= 0;
	array<int, som> arr;
	
	for (int i = 0; i <check.size(); i++)
	{
		if(!check.empty())
		{arr[i] = check.front();
		check.pop();}
	}
	
cout << arr.size() << "array";
	for (int i = 0; i <arr.size(); i++)
	{
		
		if(n == arr[i])
		{
			c++;
			break;
		}
	}
	return c;
}
int main() {
	// your code goes here
	
	int v,e,n1,n2,chk;
	queue<int> q,s;
	int i,j;
	
	while(cin >> v >> e){
		if (v != 0 || e != 0)
		{
			int arr[v][v] = {0};	
			
			for (i =0 ; i<e; i++){
				cin >> n1 >> n2;
				arr[n1][n2] = 1;
				arr[n2][n1] = 1;
			}
			
			for (i = 0; i < v ; i++){
				int c = check(q,i);
				cout << c << "r";
				if(c == 0)
					q.push(i);
				for (j = i+1; j < v ; j++){
					//cout << arr[i][j]<< " "
					if(arr[i][j] == 1)
					{
						int c = check(q,j);
						if (c == 0)
							q.push(j);
					}
				}
			
			while(!q.empty()){
				cout<< q.front() <<" ";
				q.pop();
			}
				cout << endl;
			}
		}
	}
	
	
	return 0;
}

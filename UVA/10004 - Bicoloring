#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int v,e,n1,n2;
	int i,j;
	
	while(cin >> v >> e){
		if (v != 0 || e != 0)
		{
			int arr[v][e] = {0};	
			
			for (i =0 ; i<e ; i++){
				cin >> n1 >> n2;
				arr[n1][n2] = 1;
				arr[n2][n1] = 1;
			}
			
			for (i = 0; i < v ; i++){
				for (j = 0; j < v ; j++){
					cout << arr[i][j]<< " ";
				}
				cout << endl;
			}
		}
	}
	
	
	return 0;
}

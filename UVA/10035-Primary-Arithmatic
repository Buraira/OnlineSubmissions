#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int num_a, num_b, sum, carry, case;
	int c =0;
	
	cin >> num_a >> num_b;
	
	for (int i = 0; ; i++)
	{
		sum = (num_a%10) + (num_b%10);
		
		if(sum > 9)
		{
			c++;
			carry = (sum - (sum%10))/10;
		}
		else
			carry = 0 ;
		
		num_a = num_a/10;
		num_b = num_b/10 + carry;
	
		if(num_a == 0)
			break;
	}
	return 0;
}

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int year, l,h,b;
	l=h=b=0;
	
	while (cin >> year)
	{
		
		if (year == 0)
			break;
	
		else
		{
			if ((year%4 == 0 && year%100 != 0) || year%400 == 0)
			{
				cout << "This is leap year." << endl;
				l=1;
			}
			if (year%15 == 0)
			{	
				cout << "This is huluculu festival year." << endl;
				h = 1;
			}
			if (l==1 && year%55 == 0)
			{
				cout << "This is bulukulu festival year." << endl;
				b = 1;
			}
			
			if (l==0 && h==0 && b==0)
				cout << "This is an ordinary year." << endl;
			
			cout << endl;		
			l=h=b=0;
		}		
	}	
	return 0;
}

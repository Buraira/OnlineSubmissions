#include <iostream>
#include <string>

using namespace std;

int main() {
	// your code goes here
	string s;
	int i = 0;
	
	while (getline(cin,s))
	{
		if (s == "#")
			break;
		else
		{
			if (s == "HELLO")
				cout << "Case" << i++ <<": ENGLISH";
			else if (s == "HOLA")
				cout << "Case" << i++ <<": SPANISH";
			else if (s == "HALLO")
				cout << "Case" << i++ <<": GERMAN";
			else if (s == "BONJOUR")
				cout << "Case" << i++ <<": FREANCH";
			else if (s == "CIAO")
				cout << "Case" << i++ <<": ITALIAN";
			else if (s == "ZDRAVSTVUJTE")
				cout << "Case" << i++ <<": RUSSIAN";
		}
	}
	return 0;
}

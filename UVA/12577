#include <iostream>
#include <cstdio>

using namespace std;

int
main()
{

   string type;
    for (int m = 0; ; m++)
    {
        cin >> type;
        if (type == "*")
            break;
        else
        {
           if (type == "Hajj")
            std::cout << "Case " << m+1 << ": Hajj-e-Akbar" << std::endl;
           else
            cout << "Case "<< m+1 <<": Hajj-e-Asghar\n";
        }
    }
    return 0;
}

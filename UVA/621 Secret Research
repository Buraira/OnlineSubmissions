
#include <iostream>
#include <string>

using namespace std;

int main() {
	// your code goes here
	string s;
	int i = 0;
	int c;
	
	cin >> c;
	
	while (c)
	{
		cin >> s;
		
		i = s.length();
		if (s == "4" || s == "78" || s == "1")
			cout << "+" << endl;
		
		else if ( i > 2 && s[i-2] == "3" && s[i-1] == "35" )
			cout << "-" << endl;
		else if ( i > 2 && s[0] == "9" && s[i-1] == "8" )
			cout << "*" << endl;
		else if ( i > 2 && s[0] == "1" && s[1] == "9" && s[2] == "0")
			cout << "?" << endl;
		
		c--;
	}
	return 0;
}
